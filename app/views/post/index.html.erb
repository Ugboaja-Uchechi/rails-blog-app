<%= render partial: "partials/user", locals: { user: @user } %>
<% @user.all_posts.each do |post| %>
<%= link_to 'post/' + post.id.to_s, :remote => true do %>

  <%= render partial: "partials/post", locals:{ post: post } %>
  <%= render partial: "partials/comment", locals:{ post: post } %>
  <% end %>

<% end %>